<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/relativeLayoutGuestStart"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        android:id="@+id/imgMyAccount"
        android:layout_width="wrap_content"
        android:layout_height="140dp"
        android:layout_marginBottom="33dp"
        android:layout_marginTop="65dp"
        android:src="@drawable/favicon"
        app:layout_constraintBottom_toTopOf="@+id/buttonSignUp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />
    <Button
        android:id="@+id/buttonSignIn"
        android:layout_width="277dp"
        android:layout_height="67dp"
        android:layout_alignStart="@+id/buttonSignUp"
        android:layout_below="@+id/buttonSignUp"
        android:layout_marginTop="50dp"
        android:background="@color/colorPrimary"
        android:text="@string/signIn"
        android:textAppearance="@style/TextAppearance.AppCompat.Title.Inverse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.788" />
    <Button
        android:id="@+id/buttonSignUp"
        android:layout_width="277dp"
        android:layout_height="67dp"
        android:layout_below="@+id/imgMyAccount"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="23dp"
        android:background="@color/colorPrimary"
        android:text="@string/signUp"
        android:textAppearance="@style/TextAppearance.AppCompat.Title.Inverse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.585" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/buttonSignIn"
        android:layout_marginBottom="19dp"
        android:layout_marginEnd="13dp"
        android:layout_toStartOf="@+id/buttonSignIn"
        app:srcCompat="@drawable/icons8_contacts" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonSignIn"
        android:layout_alignStart="@+id/imageView19"
        android:layout_marginBottom="17dp"
        app:srcCompat="@drawable/icons8_add_user" />
</RelativeLayout>
